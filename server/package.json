{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "server": "nodemon ./bin/www",
    "client": "npm run start --prefix ../client",
    "build": "npm install && npm --prefix ../client install",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "axios": ">=0.18.1",
    "concurrently": "^4.1.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "tcomb": "^3.2.29",
    "tcomb-validation": "^3.4.1"
  },
  "devDependencies": {
    "nodemon": "^1.19.0"
  }
}
